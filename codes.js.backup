// 验证激活码是否有效
function validateCode(code) {
    // 转换为大写进行比较（不区分大小写）
    const upperCode = code.trim().toUpperCase();
    
    // 检查激活码是否在列表中
    const isValid = ACTIVATION_CODES.some(validCode => validCode.toUpperCase() === upperCode);
    
    if (!isValid) {
        return { valid: false, message: "❌ 激活码无效，请检查后重试" };
    }
    
    // 检查激活码是否已被使用
    const usedCodes = JSON.parse(localStorage.getItem('usedCodes') || '[]');
    if (usedCodes.includes(upperCode)) {
        return { valid: false, message: "⚠️ 此激活码已被使用" };
    }
    
    return { valid: true, message: "✅ 验证成功" };
}

// 标记激活码为已使用
function markCodeAsUsed(code) {
    const upperCode = code.trim().toUpperCase();
    const usedCodes = JSON.parse(localStorage.getItem('usedCodes') || '[]');
    usedCodes.push(upperCode);
    localStorage.setItem('usedCodes', JSON.stringify(usedCodes));
}
